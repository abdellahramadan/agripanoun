                                <div class="lab-inner">
                                    <div class="lab-thumb">
                                        <img src="{{ service.image }}" alt="{{ service.title }}">
                                    </div>
                                    <div class="lab-content">
                                        <a href="{{ service|link }}"><h5>{{ service.title }}</h5></a>
                                        <p>{{ service.content|excerpt(50) }}</p>
                                    </div>
                                </div>
                            </div>
